<?xml version="1.0" encoding="UTF-8"?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.Button?>
<VBox xmlns="http://javafx.com/javafx" xmlns:fx="http://javafx.com/fxml" fx:controller="edu.virginia.sde.reviews.MyReviewsController" spacing="10" alignment="TOP_CENTER">
    <TableView fx:id="reviewsTable" prefHeight="400" prefWidth="600">
        <columns>
            <TableColumn fx:id="courseColumn" text="Course" />
            <TableColumn fx:id="ratingColumn" text="Rating" />
            <TableColumn fx:id="commentColumn" text="Comment" />
            <TableColumn fx:id="timestampColumn" text="Timestamp" />
        </columns>
    </TableView>
    <Button text="Back" onAction="#navigateToCourseSearch" />
</VBox>

